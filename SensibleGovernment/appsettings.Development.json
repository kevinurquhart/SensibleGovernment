{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "Moderation": {
    "AutoHideReportThreshold": 3,
    "AutoHideNegativeScore": -5,
    "EnableShadowBanning": true,
    "EnableKeywordFiltering": true
  }
}